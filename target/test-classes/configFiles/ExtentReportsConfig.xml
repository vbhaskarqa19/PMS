<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- report theme -->
        <!-- standard, dark -->
        <theme>dark</theme>
    
        <!-- document encoding -->
        <!-- defaults to UTF-8 -->
        <encoding>UTF-8</encoding>
        
        <!-- protocol for script and stylesheets -->
        <!-- defaults to https -->
        <protocol>https</protocol>
        
        <!-- title of the document -->
        <documentTitle>PMS Assignment</documentTitle>
        
        <!-- report name - displayed at top-nav -->
        <reportName>PMS Assignment Report</reportName>
        
        <!-- report headline - displayed at top-nav, after reportHeadline -->
        <!--  
        <reportHeadline>Headline 1</reportHeadline>
        -->
        <testViewChartLocation>false</testViewChartLocation>
         
        <!-- global date format override -->
        <!-- defaults to yyyy-MM-dd -->
        <dateFormat>dd-MMM-yyyy</dateFormat>
        
        <!-- global time format override -->
        <!-- defaults to HH:mm:ss -->
        <timeFormat>HH:mm:ss</timeFormat>
        
        <!-- custom javascript -->
        <scripts>
            <![CDATA[
                $(document).ready(function() {
                    
                });
            ]]>
        </scripts>
        
        <!-- custom styles -->
        <styles>
            <![CDATA[
                
            ]]>
        </styles>
    </configuration>
</extentreports>